{
  "expo": {
    "name": "Wechecked_app",
    "slug": "wecheckedapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c2d31"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5fdd28bc-60c4-4cc6-a7d1-7501ddc28fbf"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.karmamgmt.iwechecked",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "package": "com.karmamgmt.wechecked",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "plugins": ["@react-native-firebase/app", "@react-native-firebase/auth"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5fdd28bc-60c4-4cc6-a7d1-7501ddc28fbf"
      }
    }
  }
}
